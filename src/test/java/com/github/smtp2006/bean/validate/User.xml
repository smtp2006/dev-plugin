<?xml version="1.0" encoding="UTF-8"?>
<beans:beans
	xmlns="http://github.com/smtp2006/bean-validate"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:beans="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
        http://github.com/smtp2006/bean-validate http://github.com/smtp2006/bean-validate-1.0.xsd ">
	
	<!-- classValidator default -->
	<classValidator>
		<propertyValidators>
			<propertyValidator name="name">
				<ruleChain>
					<rule class="com.github.smtp2006.bean.validate.rule.impl.NotNullRule" />
				</ruleChain>
			</propertyValidator>
		</propertyValidators>
	</classValidator>
	
		<!-- classValidator nameLen -->
	<classValidator name="nameLen">
		<propertyValidators>
			<propertyValidator name="name">
				<ruleChain>
					<rule class="com.github.smtp2006.bean.validate.rule.impl.NotNullRule" />
				</ruleChain>
			</propertyValidator>
		</propertyValidators>

	</classValidator>
</beans:beans>
